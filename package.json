{
  "name": "datagsm-oauth-sdk-javascript",
  "version": "0.1.0",
  "description": "A simple OAuth redirection helper library for Data GSM OAuth service integration with React.",
  "license": "MIT",
  "author": "junjuny",
  "homepage": "https://github.com/junjuny0227/datagsm-oauth-sdk-javascript#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/junjuny0227/datagsm-oauth-sdk-javascript.git"
  },
  "bugs": {
    "url": "https://github.com/junjuny0227/datagsm-oauth-sdk-javascript/issues"
  },
  "keywords": [
    "oauth",
    "authentication",
    "datagsm",
    "react",
    "typescript",
    "redirect",
    "login"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "pnpm run build"
  },
  "packageManager": "pnpm@10.26.0",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.3",
    "@types/react": "^18.3.18",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "prettier": "^3.7.4",
    "react": "^18.3.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1"
  }
}
